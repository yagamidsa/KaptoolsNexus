{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "kaptools-nexus",
  "version": "0.1.0",
  "identifier": "KapTools Nexus",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "KapTools Nexus",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "minimizable": true,
        "maximizable": true
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": false
  },
  "plugins": {
    "dialog": {},
    "fs": {
      "requireLiteralLeadingDot": false
    },
    "shell": {
      "open": true
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}